{
  "manifest_version": 3,
  "name": "TestRail+",
  "version": "3.1.0",
  "icons": {
    "32": "./icons/icon.png"
  },
  "background": {
    "service_worker": "./app/background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ]
}