{
  "name": "testrail-plus",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "build-chrome": "cp manifest.chrome.json ./public/manifest.json && tsc && vite build",
    "build-firefox": "cp manifest.firefox.json ./public/manifest.json && tsc && vite build",
    "submit-chrome": "npx chrome-webstore-upload upload --source ./dist --extension-id $CHROME_EXTENSION_ID --client-id $CHROME_CLIENT_ID --client-secret $CHROME_CLIENT_SECRET --refresh-token $CHROME_REFRESH_TOKEN",
    "submit-firefox": "npx web-ext sign --channel=listed --api-key $FIREFOX_EXT_API_KEY --api-secret $FIREFOX_EXT_API_SECRET --source-dir ./dist"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/material": "^5.15.18",
    "chrome-webstore-upload-cli": "^3.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "web-ext": "^8.1.0"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.267",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}
